{
  "name": "model-decorator",
  "version": "0.1.0-3",
  "description": "Better model, Better life!",
  "main": "./src/index.js",
  "author": "Chuck Wu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d9767192/model-decorator.git"
  },
  "browserslist": [
    "Last 2 versions"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development node build/scripts/start --sourcemaps --indir demo",
    "build": "cross-env NODE_ENV=production node build/scripts/compile --indir demo",
    "build:lib": "rimraf lib && npm run build:lib:cjs && npm run build:lib:copy && npm run build:lib:umd",
    "build:lib:cjs": "cross-env NODE_ENV=production node build/scripts/cjsPublisher.js --outdir lib",
    "build:lib:copy": "node build/scripts/copyFiles.js --outdir lib",
    "build:lib:umd": "webpack --config build/umd.webpack.config.js",
    "test": "npm run test:lint && npm run test:unit",
    "test:unit": "cross-env NODE_ENV=test jest --config ./build/configs/jest.config.js",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:lint": "node build/scripts/eslint",
    "test:lint:fix": "npm run test:lint -- --fixlint",
    "test:lint:report": "npm run test:lint -- --outputlint",
    "test:coverage": "npm run test:unit -- --coverage",
    "release": "np --no-publish --any-branch"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.1.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.6.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "figures": "^2.0.0",
