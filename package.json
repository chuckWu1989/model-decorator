    "html-webpack-plugin": "^2.24.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "lodash.isempty": "^4.4.0",
    "node-sass": "^4.7.2",
    "np": "^2.18.3",
    "optimist": "^0.6.1",
    "plop": "^1.9.1",
    "postcss-loader": "^2.0.10",
    "prop-types": "^15.5.10",
    "raw-loader": "^0.5.1",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.6.2",
    "react-hot-loader": "^3.1.3",
    "react-intl": "^2.3.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
